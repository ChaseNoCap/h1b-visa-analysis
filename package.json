{
  "name": "h1b-visa-analysis",
  "version": "1.0.0",
  "description": "H1B visa analysis meta repository",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && node dist/src/index.js",
    "build:watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "update-deps": "npm update",
    "submodule:update": "git submodule update --remote --merge",
    "submodule:init": "git submodule update --init --recursive"
  },
  "keywords": [
    "h1b",
    "report",
    "generator",
    "meta-repository"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@chasenocap/cache": "^1.0.5",
    "@chasenocap/di-framework": "^1.0.0",
    "@chasenocap/event-system": "^1.0.5",
    "@chasenocap/file-system": "^1.0.0",
    "@chasenocap/logger": "^1.0.1",
    "@chasenocap/markdown-compiler": "^0.1.1",
    "@chasenocap/report-components": "^0.1.0",
    "@chasenocap/report-templates": "^1.0.2",
    "inversify": "^6.0.2",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@chasenocap/prompts": "^1.0.0",
    "@chasenocap/test-helpers": "^0.1.0",
    "@chasenocap/test-mocks": "^0.1.1",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "globals": "^16.2.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vitest": "^3.0.0"
  }
}
